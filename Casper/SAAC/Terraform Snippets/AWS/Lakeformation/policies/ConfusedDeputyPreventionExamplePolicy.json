{
    "statement": [
        {
            "actions": [
                "sts:AssumeRole"
            ],
            "condition": [
                {
                    "test": "ForAnyValue:StringEquals",
                    "values": "${lakeformation_iam_role_caller_identity_account_id}",
                    "variable": "aws:SourceAccount"
                },
                {
                    "test": "ForAnyValue:ArnEquals",
                    "values": "${lakeformation_iam_role_caller_identity_account_arn}",
                    "variable": "aws:SourceArn"
                }
            ],
            "effect": "Allow",
            "principals": [
                {
                    "identifiers": [
                        "lakeformation.amazonaws.com"
                    ],
                    "type": "Service"
                }
            ],
            "sid": "ConfusedDeputyPreventionExamplePolicy"
        }
    ]
}
