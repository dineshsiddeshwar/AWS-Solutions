{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "sns:AddPermission",
        "sns:RemovePermission",
        "sns:CheckIfPhoneNumberIsOptedOut",
        "sns:Create*",
        "sns:Delete*",
        "sns:Publish",
        "sns:Set*",
        "sqs:*"
      ],
      "Effect": "Deny",
      "Resource": [
        "arn:aws:sns:*:*:platform*",
        "arn:aws:sqs:*:*:platform*"
      ],
      "Condition": {
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/platform*",
            "arn:aws:iam::*:role/aws-reserved/sso.amazonaws.com/*platform*",
            "arn:aws:iam::*:role/*AWSControlTower*",
            "arn:aws:iam::master_account_id:role/aws-service-role/stacksets.cloudformation.amazonaws.com/AWSServiceRoleForCloudFormationStackSetsOrgAdmin*",
            "arn:aws:iam::*:role/stacksets-exec*",
            "arn:aws:iam::*:role/aws-controltower*"
          ]
        }
      }
    },
    {
      "Effect": "Deny",
      "Action": [
        "ssm:DeleteParameter",
        "ssm:DeleteParameters",
        "ssm:PutParameter",
        "ssm:DeleteDocument",
        "ssm:UpdateDocument",
        "ssm:CreateDocument",
        "ssm:UpdateDocumentDefaultVersion"
      ],
      "Resource": [
        "arn:aws:ssm:*:*:*/Platform-*",
        "arn:aws:ssm:*:*:*/platform-*",
        "arn:aws:ssm:*:*:platform*",
        "arn:aws:ssm:*:*:document/ASR-*",
        "arn:aws:ssm:*:*:parameter/Solutions/SO0111/*"
      ],
      "Condition": {
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/platform*",
            "arn:aws:iam::*:role/aws-reserved/sso.amazonaws.com/*platform*",
            "arn:aws:iam::*:role/*AWSControlTower*",
            "arn:aws:iam::master_account_id:role/aws-service-role/stacksets.cloudformation.amazonaws.com/AWSServiceRoleForCloudFormationStackSetsOrgAdmin*",
            "arn:aws:iam::*:role/stacksets-exec*",
            "arn:aws:iam::*:role/aws-controltower*"
          ]
        }
      }
    },
    {
      "Sid": "Blockccreatelogin",
      "Effect": "Deny",
      "Action": "iam:CreateLoginProfile",
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "VisualEditor23",
      "Effect": "Deny",
      "Action": [
        "iam:DeletePolicy",
        "iam:CreateRole",
        "iam:RemoveRoleFromInstanceProfile",
        "iam:DeleteInstanceProfile"
      ],
      "Resource": [
        "arn:aws:iam::*:policy/platform_*",
        "arn:aws:iam::*:role/platform*",
        "arn:aws:iam::*:instance-profile/platform*"
      ],
      "Condition": {
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/platform*",
            "arn:aws:iam::*:role/aws-reserved/sso.amazonaws.com/*platform*",
            "arn:aws:iam::*:role/*AWSControlTower*",
            "arn:aws:iam::master_account_id:role/aws-service-role/stacksets.cloudformation.amazonaws.com/AWSServiceRoleForCloudFormationStackSetsOrgAdmin*",
            "arn:aws:iam::*:role/stacksets-exec*",
            "arn:aws:iam::*:role/aws-controltower*"
          ]
        }
      }
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Deny",
      "Action": [
        "iam:DeleteRole",
        "iam:UpdateRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:UpdateAssumeRolePolicy",
        "iam:CreatePolicyVersion"
      ],
      "Resource": [
        "arn:aws:iam::*:role/platform*",
        "arn:aws:iam::*:role/*Wiz*",
        "arn:aws:iam::*:role/*AWSReservedSSO*",
        "arn:aws:iam::*:role/SO0111*",
        "arn:aws:iam::*:role/business_service*"
      ],
      "Condition": {
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/platform*",
            "arn:aws:iam::*:role/aws-reserved/sso.amazonaws.com/*platform*",
            "arn:aws:iam::*:role/*AWSControlTower*",
            "arn:aws:iam::master_account_id:role/aws-service-role/stacksets.cloudformation.amazonaws.com/AWSServiceRoleForCloudFormationStackSetsOrgAdmin*",
            "arn:aws:iam::*:role/stacksets-exec*",
            "arn:aws:iam::*:role/aws-controltower*"
          ]
        }
      }
    }
  ]
}